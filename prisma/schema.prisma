generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgres"
  url      = env("DATABASE_URL")
}

model Users {
  id String @id() @default(uuid())
  username String
  email String @unique
  password String
  token String?
  refresh_token String?
  created_at DateTime @default(now())
  updated_at DateTime @default(now())
}